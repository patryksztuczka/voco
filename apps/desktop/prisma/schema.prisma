// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
}

model Transcription {
  id        String   @id @default(uuid())
  text      String
  timestamp DateTime @default(now())
  duration  Int?
}
